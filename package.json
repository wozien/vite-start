{
  "name": "vite-start",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:uat": "vue-tsc --noEmit && vite build --mode uat",
    "serve": "vite preview",
    "lint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "vant": "^3.0.8",
    "vue": "^3.0.5",
    "vue-router": "^4.0.4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.4",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vitejs/plugin-vue-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.21.0",
    "eslint-plugin-vue": "^7.7.0",
    "less": "^4.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.5",
    "vite-plugin-mock": "^2.2.0",
    "vite-plugin-style-import": "^0.8.1",
    "vue-tsc": "^0.0.12"
  }
}
